using System;

class Program
{
    static void Main()
    {
        double X = 1.234; // Пример значения для X
        double Y = 5.678; // Пример значения для Y
        
        Console.WriteLine(SumFractionalParts(X, Y));
    }

    static double SumFractionalParts(double x, double y)
    {
        // Приводим числа к общему знаменателю, если это необходимо
        double commonDenominator = Math.Max(x, y);
        x = x * (commonDenominator / x);
        y = y * (commonDenominator / y);
        
        // Вычисляем дробную часть каждого числа
        double fractionalPartX = x - Math.Floor(x);
        double fractionalPartY = y - Math.Floor(y);
        
        // Суммируем дробные части
        return fractionalPartX + fractionalPartY;
    }
}